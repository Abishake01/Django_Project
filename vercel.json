{
    "version": 2,
    "builds": [
      {
        "src": "calculator/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime":"python" 
        }  
       },
       {
        "src":"build_files.sh",
        "use":"@vercel/static-build",
        "config":{
            "distDir":"staticfiles_build"
        }
       }
    ],
    "routes": [
    {
            "src": "/(.*)",
            "dest":"/static/$1"
    },
      {
        "src": "/(.*)",
        "dest": "calculator/wsgi.py"
      }
    ]
  }